@model Post
@{
    ViewData["Title"] = "Details";
    Layout = "~/Areas/Admin/Shared/_Layout.cshtml";
}

<h1>Detail</h1><br /><br />

<div class="col-4">
	<img src="~/images/@Model.Photo" height="100px" /><br /><br />
	<h3>Title: @Model.Title </h3>
	<h4>Description:@Model.Description </h4><br />
	<a class="btn btn-info" asp-action="Index">Back</a>
</div

